{
  "name": "traces",
  "version": "1.0.0",
  "description": "Back-end VSCC trace application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npx nodemon --watch app -e js,ts,json --exec \"tsx index.ts\"",
    "lint": "eslint --fix .",
    "prepare": "husky",
    "test": "jest",
    "start": "node dist/index.js",
    "build": "tsup index.ts --format esm --clean",
    "db": "npm run db:create && npm run db:seeding",
    "db:create": "psql -d vscc -f app/data/create.structure.sql",
    "db:seeding": "psql -d vscc -f app/data/seeding.fakers.sql"
  },
  "keywords": [
    "template",
    "Express",
    "Jest",
    "supertest",
    "server",
    "Joi",
    "ESLint",
    "Winston",
    "EJS",
    "CORS",
    "Helmet",
    "Limiter",
    "JWT",
    "Bcrypt"
  ],
  "author": "Romuald Patfoort",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.4",
    "@types/pg": "^8.11.10",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/nodemailer": "^6.4.16",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.15",
    "pg": "^8.11.3",
    "tsx": "^4.19.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
